{
  "name": "vite-plugin-build-size-report",
  "version": "1.0.0",
  "description": "Vite plugin for generate build size report",
  "type": "module",
  "scripts": {
    "lint": "standard",
    "build": "tsup build src/index.ts --format esm,cjs --dts"
  },
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "vite",
    "plugin",
    "report",
    "build",
    "size"
  ],
  "author": "kontur.io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "filesize": "^6.1.0",
    "webpack-stats-diff": "^1.3.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.14.0",
    "tsup": "^6.6.3",
    "standard": "^16.0.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
